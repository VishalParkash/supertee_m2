     <script type="text/javascript">
        require(['jquery', 'bootstrap4'], function($) {
            // console.log('hhh');
            var headerElem = $('.panel.header');
            var loginLink = $('.topHeader').find('#login-popup');
            var registerLink = headerElem.find('.authorization-link').next().children('a');
            var loginOverlay = $('.login-overlay');
            var loginBox = $('.tab-content').find('.popup-login-form');
            var registerBox = $('.tab-content').find('.popup-register-form');
            var loginButton = $('.login-form').find('button[type=submit]');
            var registerButton = $('.register-form').find('button[type=submit]');
            var forgetButton = $('.forget-form').find('button[type=submit]');
            
            var loginUrl = "<?php echo $block->getUrl('customerlogin/ajax/login/') ?>";
            var registerUrl = "<?php echo $block->getUrl('customerlogin/ajax/register/')?>";
            var forgetUrl = "<?php echo $block->getUrl('customerlogin/ajax/forget/')?>";
            // var loginUrl = "https://dev.evantiv.com/supertee/customerlogin/ajax/login/";

            // var registerUrl = "https://dev.evantiv.com/supertee/customerlogin/ajax/register/";
            
            // var forgetUrl = "https://dev.evantiv.com/supertee/customerlogin/ajax/forget/";

            // Texts
            var loginText = "Login";
            var loggingText = "Please Wait...";

            var registerText = "Sign Up";
            var registeringText = "Please Wait...";

            var forgetsText = "Forget";
            var forgetText = "Please Wait...";

            loginLink.click(function(e){
                console.log('login clicked');
                e.preventDefault(e);
                loginOverlay.show();
                loginBox.show();
                e.stopPropagation(); 
                var target = $(this).attr('data-target');
                $(target).addClass('show d-block');
                $('body').addClass("modal-open");
                $('body').append('<div class="modal-backdrop fade show"></div>');
            });

            registerLink.click(function(e){
                e.preventDefault(e);
                loginOverlay.show();
                registerBox.show();
            });

            $('.cancelbtn').click(function(){
                loginOverlay.hide();
                loginBox.hide();
                registerBox.hide();
            });

            $('.login-link a').click(function(e){
                e.preventDefault(e);
                loginBox.show();
                registerBox.hide();
            });

            $('.register-link a').click(function(e){
                e.preventDefault(e);
                loginBox.hide();
                registerBox.show();
            });

            $('.login-form').submit(function(){
                loginButton.text(loggingText);
                loginButton.attr("disabled", "disabled");
                var formData = getFormData($(this));
                $.ajax({
                    type: "POST",
                    url: loginUrl,
                    data: formData,
                    dataType: "json",
                    success: function(data) {
                        console.log(data);
                        showResponse(data);
                        if(data.errors) {
                            loginButton.text(loginText);
                            loginButton.removeAttr('disabled');
                        } else {
                            if(data.result == 'client'){
                                window.location.href = "<?php echo $this->getUrl()?>client/client/index";
                            }else{
                                location.reload();
                            }
                            // location.reload();
                        }
                    }
                });
                return false;
            });

            $('.register-form').submit(function(){
                registerButton.text(registeringText);
                registerButton.attr("disabled", "disabled");
                var formData = getFormData($(this));
               // console.log(formData);
                $.ajax({
                    type: "POST",
                    url: registerUrl,
                    data: formData,
                    dataType: "json",
                    success: function(data) {
                        console.log(data);
                        showResponse(data);
                        if(data.errors) {
                            registerButton.text(registerText);
                            registerButton.removeAttr('disabled');
                        } else {
                            location.reload();
                        }
                    }
                });
                return false;
            });

            $('.forget-form').submit(function(){
                forgetButton.text(forgetText);
                forgetButton.attr("disabled", "disabled");
                var formData = getFormData($(this));
               //console.log(formData);
                $.ajax({
                    type: "POST",
                    url: forgetUrl,
                    data: formData,
                    dataType: "json",
                    success: function(data) {
                        console.log(data);
                        showResponse(data);
                        if(data.errors) {
                            forgetButton.text(forgetsText);
                            forgetButton.removeAttr('disabled');
                        } else {
                            //console.log('here we are');
                            forgetButton.text(forgetsText);
                            forgetButton.removeAttr('disabled');
                            //location.reload();
                            resetLinkModal();
                        }
                    }
                });
                return false;
            });

            function getFormData(formElem){
                var unindexed_array = formElem.serializeArray();
                var indexed_array = {};

                jQuery.map(unindexed_array, function(n, i){
                    indexed_array[n['name']] = n['value'];
                });

                return indexed_array;
            }

            function showResponse(data) {
                // console.log(data);
                // console.log("data");
                if(data.errors) {
                    console.log(data.message);
                    $('.response-msg').html("<div class='notify error'><h6>"+data.message+"</h6></div>");
                } else {
                    console.log(data.message);
                    $('.response-msg').html("<div class='notify success'><h6>"+data.message+"</h6></div>");
                }
                setTimeout(function(){ $('.response-msg').html(null); }, 5000);
            }

            // resetlink modal
            function resetLinkModal(e){
                 // e.stopPropagation(); 
                $('.login_process').removeClass('show d-block');
                $('#resetLink').addClass('show d-block');
             }
            
            



            // updated login
            // signin modal
            $('#modalOpen').on('click', function(e){
                var target = $(this).attr('data-target');

                console.log(target);
                e.stopPropagation(); 
                $(target).addClass('show d-block');
                $('body').addClass("modal-open");
                $('body').append('<div class="modal-backdrop fade show"></div>');

                
            });

            // reward_donation_signup
            $('.rwd_main #modalOpen').on('click', function(e){
                var target = $(this).attr('data-target');
                var viewId = $(this).attr('view-id');
                console.log(viewId);

                e.stopPropagation(); 
                $(target).addClass('show d-block');

                $('.login_process .nav-tabs .nav-item a').removeClass('active');
